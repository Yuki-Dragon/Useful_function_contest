    "    \n",
    "    if N==0:\n",
    "        xans = x\n",
    "        return xans\n",
    "    \n",
    "        \n",
    "    \n",
    "    xans = r*Linear_Recursive(x, r, D, N-1) - D # linear eq.\n",
    "    \n",
    "    return xans"
