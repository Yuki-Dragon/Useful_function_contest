    "import numpy as np\n",
    "def Binary_Gen(n):\n",
    "    bina = np.ones(2**n,dtype=object)\n",
    "    for i in range(2**n):\n",
    "        b = str(bin(i))[2:].zfill(n)\n",
    "        bina[i] = b\n",
    "    return bina"
   ]
